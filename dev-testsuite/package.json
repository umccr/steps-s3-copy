  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.954.0",
    "@aws-sdk/client-s3": "3.954.0",
    "@aws-sdk/client-sfn": "3.954.0",
    "@aws-sdk/client-sts": "3.954.0",
    "@aws-sdk/crc64-nvme-crt": "3.954.0",
    "@smithy/util-waiter": "4.2.6",
    "aws-crt": "1.28.1",
    "crc": "4.3.2",
    "p-limit": "7.2.0"
  },
  "devDependencies": {
    "@tsconfig/bun": "1.0.10",
    "@types/bun": "1.3.4",
    "typescript": "5.9.3"
  }
