{
  "name": "dev-testsuite",
  "private": true,
  "version": "0.0.0",
  "description": "A test suite that runs on a dev deployed version of Steps S3 Copy",
  "type": "module",
  "scripts": {
    "e2e-dryrun": "bun test ./test-e2e-dryrun.ts",
    "e2e-koalas": "bun test ./test-e2e-koalas.ts",
    "e2e-realistic": "bun test ./test-e2e-realistic.ts",
    "e2e-thawing": "bun test ./test-e2e-thawing.ts",
    "unit-step-can-write": "bun test --timeout 30000 ./test-step-can-write-lambda.ts"
